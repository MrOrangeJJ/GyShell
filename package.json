{
  "name": "gyshell",
  "version": "0.1.1",
  "description": "GyShell - AI-Native macOS Shell App",
  "main": "./out/main/index.js",
  "author": "TUOTUO",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "build": "electron-vite build",
    "dist": "electron-vite build && electron-builder",
    "dist:mac": "export CSC_IDENTITY_AUTO_DISCOVERY=false && electron-vite build && electron-builder --mac --dir && bash scripts/fix-mac-signatures.sh && electron-builder --mac --prepackaged dist/mac-arm64",
    "dist:win": "electron-vite build && electron-builder --win --x64",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "lint": "eslint .",
    "format": "prettier --write .",
    "rebuild:native": "electron-rebuild -f",
    "postinstall": "electron-builder install-app-deps && npm run rebuild:native"
  },
  "dependencies": {
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/mcp-adapters": "^1.1.1",
    "@langchain/openai": "^1.2.0",
    "@modelcontextprotocol/sdk": "^1.2.1",
    "@types/ssh2": "^1.15.5",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-search": "^0.16.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/addon-webgl": "^0.19.0",
    "@xterm/headless": "^6.0.0",
    "@xterm/xterm": "^6.0.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "diff": "^8.0.3",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.6.0",
    "i18next": "^25.7.4",
    "langchain": "^1.2.3",
    "lucide-react": "^0.562.0",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "node-pty": "1.2.0-beta.3",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.2",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "remark-gfm": "^4.0.1",
    "remeda": "^2.33.4",
    "socks": "^2.8.7",
    "ssh2": "^1.17.0",
    "tree-sitter-bash": "^0.25.1",
    "uuid": "^9.0.0",
    "web-tree-sitter": "^0.26.3",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "sass": "^1.91.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}
